{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
  {
      "path": "pages/carList/carList",
      "style": {
        
          "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/index"
    }, {
      "path": "pages/user/user",
      "style": {
        "enablePullDownRefresh": true
      }
    }, {
      "path": "pages/careerMaps/careerMaps",
      "style": {
        "enablePullDownRefresh": true
      }
    },  {
      "path": "pages/dashboard/dashboard",
      "style": {
        "navigationBarTitleText": "控制台"
      }
    }

    , {
      "path": "pages/dashboard/contest/insert/insert",
      "style": {}
    }
    ],
  "globalStyle": {
    //为支持微信深色模式，此处样式转移到manifest.json中
  },
  "tabBar": {
    "color": "@tabFontColor",
    "backgroundColor": "@tabBgColor",
    "selectedColor": "@tabSelectedColor",
    "list": [
      {
        "pagePath": "pages/carList/carList",
        "iconPath": "static/tabbar-icons/car.png",
        "selectedIconPath": "static/tabbar-icons/car-selected.png",
        "text": "车辆"
      },
      {
        "pagePath": "pages/careerMaps/careerMaps",
        "iconPath": "static/tabbar-icons/flag.png",
        "selectedIconPath": "static/tabbar-icons/flag-selected.png",
        "text": "地图"
      },
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/tabbar-icons/fire.png",
        "selectedIconPath": "static/tabbar-icons/fire-selected.png",
        "text": "赛事"
      }
      // ,
      // {
      //   "pagePath": "pages/user/user",
      //   "iconPath": "static/tabbar-icons/user.png",
      //   "selectedIconPath": "static/tabbar-icons/user-selected.png",
      //   "text": "我的"
      // }
    ]
  }
}
