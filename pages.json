  "globalStyle": {
    //为支持微信深色模式，此处样式转移到manifest.json中
    // #ifdef  MP-WEIXIN
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarBackgroundColor": "@navBgColor",
    "backgroundColor": "@bgColor",
    // #endif
    // #ifdef MP-QQ
    "navigationBarTextStyle": "black",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#F5F5F5",
    // #endif
    "navigationBarTitleText": "狂飙小助手"
  },

  "tabBar": {
    // #ifdef  MP-WEIXIN
    "color": "@tabFontColor",
    "backgroundColor": "@tabBgColor",
    "selectedColor": "@tabSelectedColor",
    // #endif
    // #ifdef MP-QQ
    "color": "#000",
    "backgroundColor": "#fff",
    "selectedColor": "#FF0054",
    // #endif


    // #ifdef  MP-WEIXIN
    "list": [{
        "pagePath": "pages/carList/carList",
        "iconPath": "@iconPathCarList",
        "selectedIconPath": "@selectedIconPathCarList",
        "text": "车辆"
      },
      {
        "pagePath": "pages/careerMaps/careerMaps",
        "iconPath": "@iconPathCareerMaps",
        "selectedIconPath": "@selectedIconPathCareerMaps",
        "text": "地图"
      },
      {
        "pagePath": "pages/index/index",
        "iconPath": "@iconPathContest",
        "selectedIconPath": "@selectedIconPathContest",
        "text": "赛事"
      }
    ]
    // #endif

